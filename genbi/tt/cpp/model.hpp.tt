[% IF model.is_block('transition') %]
template<class T1>
inline bool [% class_name %]::onDelta(const T1 t1, const T1 t2) {
  T1 sgn = (t2 >= t1) ? 1.0 : -1.0;
  T1 nextDelta = sgn*ge_step(t1, sgn*getDelta());
  return sgn*t1 >= sgn*nextDelta;
}
