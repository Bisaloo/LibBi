Description="LibBi config files"

Keywords={
  {  Id=1,
     Regex=[[(\-\-?[a-zA-Z][a-zA-Z0-9\-_]*)]]
  },
  {  Id=2,
     Regex=[[(\@\w+\.conf)]]
  }
}

Strings = {
  Delimiter=[["|']],
  RawPrefix="R",
}

Comments = {
   {
     Block=false,
     Delimiter = { [[\#]] }
   }
}

IgnoreCase=false
EnableIndentation=true
