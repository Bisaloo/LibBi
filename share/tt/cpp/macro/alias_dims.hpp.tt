[%-MACRO alias_dims(action) BLOCK %]
  [%-un = 0-%]
  [%-FOREACH alias IN action.get_aliases %]
  [%-IF alias.has_name -%]
  BI_UNUSED const int [% alias.get_name %]_ = cox.i[% loop.index %];
  [%-ELSE-%]
  BI_UNUSED const int un[% un; un = un + 1 %] = cox.i[% loop.index %];  
  [%-END-%]
  [%-END %]
[%-END-%]
